{
  "job_config": {
    "compile_cc": {
      "flags": [
        "-Ofast",
        "-flto",
        "-march=native",
        "-mtune=native",
        "-DNDEBUG",
        "-Wno-unused",
        "-Wno-unused-parameter",
        "-Wno-unused-result",
        "-gline-tables-only",
        "-fstrict-vtable-pointers"
      ]
    },
    "link": {
      "flags": [
        "-Ofast",
        "-flto=thin",
        "-fuse-ld=lld",
        "-fwhole-program",
        "-march=native",
        "-mtune=native",
        "-rdynamic",
        "-fstrict-vtable-pointers",
        "-strip"
      ]
    }
  }
}
