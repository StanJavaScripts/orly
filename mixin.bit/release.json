{
  "job_config": {
    "compile_cc": {
      "flags": [
        "-Ofast",
        "-flto=thin",
        "-march=native",
        "-mtune=native",
        "-DNDEBUG",
        "-Wno-unused",
        "-Wno-unused-parameter",
        "-Wno-unused-result",
        "-fstrict-vtable-pointers"
      ]
    },
    "link": {
      "flags": [
        "-Ofast",
        "-flto=thin",
        "-fwhole-program",
        "-march=native",
        "-mtune=native",
        "-rdynamic",
        "-fstrict-vtable-pointers",
        "-fno-omit-frame-pointer",
        "-s"
      ]
    }
  }
}